{
  "name": "coupleringweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run lint && tsc -b && vite",
    "style": "npx typed-scss-modules src --outputFolder style --watch",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 3",
    "watch": "esw src --watch --ext .ts,.tsx --report-unused-disable-directives --max-warnings 3 --color --clear",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.5",
    "@mui/material": "^5.16.5",
    "@mui/x-data-grid": "^7.11.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@tanstack/react-query": "^5.51.15",
    "axios": "^1.7.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "react-text-transition": "^3.1.0",
    "react-toastify": "^10.0.5",
    "redux-persist": "^6.0.0",
    "sass": "^1.77.8",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.7.5",
    "turndown": "^7.2.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/slick-carousel": "^1.6.40",
    "@types/turndown": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-watch": "^8.0.0",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-checker": "^0.8.0"
